    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".jugador.viewActivities.fragments.CampeonatoIntercentro.IntercentrosCampoenato">

    <androidx.recyclerview.widget.RecyclerView
        android:layout_marginTop="80dp"
        android:id="@+id/recyclerViewCampeonatosintercentros"
